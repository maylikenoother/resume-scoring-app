{
  "name": "cv-review-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "cd api && pip install -r ../requirements.txt && uvicorn main:app --reload",
    "next-dev": "next dev",
    "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "cd api && pytest",
    "db:migrate": "cd api && alembic upgrade head",
    "db:migrate:create": "cd api && alembic revision --autogenerate -m",
    "db:migrate:rollback": "cd api && alembic downgrade -1",
    "db:reset": "cd api && python create_tables.py --reset"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.17.1",
    "@mui/material": "^5.17.1",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "next": "^15.3.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/next": "^9.0.0",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.4",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.3.3"
  }
}