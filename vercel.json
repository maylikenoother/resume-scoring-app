{
    "version": 2,
    "builds": [
      { "src": "api/index.py", "use": "@vercel/python" },
      { "src": "package.json", "use": "@vercel/next" }
    ],
    "routes": [
      { "src": "/api/py/(.*)", "dest": "api/index.py" },
      { "src": "/(.*)", "dest": "/$1" }
    ],
    "env": {
      "PYTHONPATH": "."
    }
  }